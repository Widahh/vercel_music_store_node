{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/src/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/src/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/js/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/src/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/pages/index.html"
    }
  ]
}
